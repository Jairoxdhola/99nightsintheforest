WeaponApply.MouseButton1Click:Connect(function()
    local weaponName = WeaponInput.Text
    local weaponArg = ArgInput.Text

    if not weaponName or weaponName == OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({}) then
        notify(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({10605,11970,11970,11655,11970}), OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({7245,11340,3360,11550,11655,11445,10290,11970,10605,3360,10500,10605,11340,3360,10185,11970,11445,10185,3360,11550,11655,3360,11760,12285,10605,10500,10605,3360,10605,12075,12180,10185,11970,3360,12390,10185,10395,20475,18165,11655,4830}))
        return
    end

    local i = LocalPlayer:FindFirstChild(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({7665,11550,12390,10605,11550,12180,11655,11970,12705}))
    if i and i:FindFirstChild(weaponName) then
        Config.KillAura.WeaponName = weaponName
        Config.KillAura.StaticArg3 = weaponArg
        notify(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({12075,12285,10395,10395,10605,12075,12075}), OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({6825,11970,11445,10185,3360,10185,10395,12180,12285,10185,11340,11025,12810,10185,10500,10185,3360,10185,3360})OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({3360,4830,4830,3360,12495,10605,10185,11760,11655,11550,8190,10185,11445,10605,3360,4830,4830,3360})OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({}))

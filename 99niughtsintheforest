task.spawn(function()
    local remoteEvent = ReplicatedStorage:WaitForChild(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180,12075}))
        :WaitForChild(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({8820,11655,11655,11340,7140,10185,11445,10185,10815,10605,8295,10290,11130,10605,10395,12180}))
    while true do
        task.wait(Config.KillAura.Interval)
        if Config.KillAura.IsEnabled then
            local char = LocalPlayer.Character
            local root = char and char:FindFirstChild(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
            local inv = LocalPlayer:FindFirstChild(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({7665,11550,12390,10605,11550,12180,11655,11970,12705}))
            local weapon = inv
                and inv:FindFirstChild(Config.KillAura.WeaponName)
            if not (root and weapon) then
                if Config.KillAura.IsEnabled then
                    Config.KillAura.IsEnabled = false
                    AuraToggle.Text = OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({25200,16695,15540,18900,3360,6825,8925,8610,6825,3360,7140,7245,8715,6825,7035,8820,7665,9030,6825,7140,6825})
                    AuraToggle.BackgroundColor3 = Config.Theme.Error
                    notify(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({12495,10185,11970,11550,11025,11550,10815}), OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({6825,12285,11970,10185,3360,10500,10605,12075,10185,10395,12180,11025,12390,10185,10500,10185,4620,3360,10185,11970,11445,10185,3360,11760,10605,11970,10500,11025,10500,10185,4830}))
                end
                continue
            end
            for _, target in ipairs(Config.KillAura.TargetFolder:GetChildren()) do
                if target:IsA(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({8085,11655,10500,10605,11340})) and target ~= char then
                    local h = target:FindFirstChildOfClass(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({7560,12285,11445,10185,11550,11655,11025,10500}))
                    local tr = target:FindFirstChild(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
                    if
                        h
                        and tr
                        and h.Health > 0
                        and (root.Position - tr.Position).Magnitude
                            <= Config.KillAura.Range
                    then
                        pcall(function()
                            remoteEvent:InvokeServer(
                                target,
                                weapon,
                                Config.KillAura.StaticArg3, -- Ahora usa el valor guardado
                                tr.CFrame
                            )
                        end)
                    end
                end
            end
        end
    end
end)

local isOpen = false
local function toggleMenu()
    isOpen = not isOpen
    local targetPos = isOpen and UDim2.new(0.5, -250, 0.5, -260)
        or UDim2.new(0.5, -250, 1.5, 0)
    local tweenInfo =
        TweenInfo.new(0.5, Enum.EasingStyle.Quart, Enum.EasingDirection.Out)
    if isOpen then
        MainFrame.Visible = true
    end
    local tween =
        TweenService:Create(MainFrame, tweenInfo, { Position = targetPos })
    tween.Completed:Connect(function()
        if not isOpen then
            MainFrame.Visible = false
        end
    end)
    tween:Play()
end

CloseButton.MouseButton1Click:Connect(toggleMenu)

--// ======================== GESTI√ìN DE ENTRADA (PC & M√ìVIL) ========================
if UserInputService.TouchEnabled then
    -- Es un dispositivo m√≥vil, crear un bot√≥n dedicado para abrir/cerrar el men√∫.
    local MobileToggleButton = Instance.new(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({8820,10605,12600,12180,6930,12285,12180,12180,11655,11550}), ScreenGui)
    MobileToggleButton.Name = OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({8085,11655,10290,11025,11340,10605,8820,11655,10815,10815,11340,10605})
    MobileToggleButton.Size = UDim2.new(0, 60, 0, 60)
    MobileToggleButton.Position = UDim2.new(1, -75, 1, -75) -- Esquina inferior derecha
    MobileToggleButton.ZIndex = 10
    MobileToggleButton.Text = OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({23730,15960,18480})
    MobileToggleButton.Font = Enum.Font.GothamBold
    MobileToggleButton.TextSize = 32
    MobileToggleButton.TextColor3 = Config.Theme.Text
    MobileToggleButton.BackgroundColor3 = Config.Theme.Primary
    MobileToggleButton.AutoButtonColor = false

    local mtCorner = Instance.new(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({8925,7665,7035,11655,11970,11550,10605,11970}), MobileToggleButton)
    mtCorner.CornerRadius = UDim.new(0, 12)
    local mtStroke = Instance.new(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({8925,7665,8715,12180,11970,11655,11235,10605}), MobileToggleButton)
    mtStroke.Color = Config.Theme.Text
    mtStroke.Thickness = 1.5

    -- Conectar el bot√≥n m√≥vil a la funci√≥n de abrir/cerrar
    MobileToggleButton.MouseButton1Click:Connect(toggleMenu)
else
    -- Es PC, usar la tecla OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({7455}) como antes.
    UserInputService.InputBegan:Connect(function(input, gp)
        if gp or UserInputService:GetFocusedTextBox() then
            return
        end
        if input.KeyCode == Enum.KeyCode.G then
            toggleMenu()
        end
    end)
end

local function Initialize()
    notify(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({12075,12285,10395,10395,10605,12075,12075}), OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({6930,11025,10605,11550,12390,10605,11550,11025,10500,11655,4620,3360}) .. LocalPlayer.DisplayName .. OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({3465}))
    
    -- Muestra la notificaci√≥n de c√≥mo abrir el men√∫ seg√∫n la plataforma
    if UserInputService.TouchEnabled then
        notify(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({11025,11550,10710,11655}), OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({8820,11655,10395,10185,3360,10605,11340,3360,10290,11655,12180,20475,18795,11550,3360,23730,15960,18480,3360,11760,10185,11970,10185,3360,10185,10290,11970,11025,11970,3360,10605,11340,3360,11445,10605,11550,20475,19530,4830}))
    else
        notify(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({11025,11550,10710,11655}), OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({8400,11970,10605,12075,11025,11655,11550,10185,3360,9555,7455,9765,3360,11760,10185,11970,10185,3360,10185,10290,11970,11025,11970,3360,10605,11340,3360,11445,10605,11550,20475,19530,4830}))
    end

    local itemsFolder = Workspace:FindFirstChild(Config.ItemsList.FolderName)
    if itemsFolder then
        itemsFolder.ChildAdded:Connect(onItemAdded)
        itemsFolder.ChildRemoved:Connect(onItemRemoved)
        for _, item in ipairs(itemsFolder:GetChildren()) do
            onItemAdded(item)
        end
        notify(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({12075,12285,10395,10395,10605,12075,12075}), #itemsFolder:GetChildren() .. OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({3360,11025,12180,10605,11445,12075,3360,10395,10185,11970,10815,10185,10500,11655,12075,4830}))
    else
        notify(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({12495,10185,11970,11550,11025,11550,10815}), OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({7035,10185,11970,11760,10605,12180,10185,3360,10500,10605,3360,11025,12180,10605,11445,12075,3360,11550,11655,3360,10605,11550,10395,11655,11550,12180,11970,10185,10500,10185,4830}))
    end
    switchTab(OtLOkiFlQqNvXznbMYpEwgOWiMfauMVqxxSBXHcgA({6930,11970,11025,11550,10815}))
    task.spawn(function()
        while MainFrame and MainFrame.Parent do
            for i = 0, 360, 2 do
                MainGradient.Rotation = i
                task.wait()
            end
        end
    end)
end

pcall(Initialize)
        
